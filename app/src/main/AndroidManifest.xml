<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bankingapplication">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CardInfo"></activity>
        <activity android:name=".AccountInfo" />
        <activity android:name=".AllCardsAccounts" />
        <activity android:name=".UserInfo" />
        <activity android:name=".UserCreation" />
        <activity android:name=".LoginActivity" />
        <activity
            android:name=".CurrencyConverter"
            android:label="Currency Converter" />
        <activity
            android:name=".BankLocations"
            android:label="Bank Locations" />
        <activity
            android:name=".Notifications"
            android:label="@string/title_activity_notifications" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>